package array_based_problem;

import java.util.Scanner;

public class SumOfEven {

	public static void main(String[] args) {
			Scanner sc = new Scanner(System.in);
			int n = sc.nextInt();
			int sum =0;
			int arr[] = new int[n];
			for(int i = 0;i<n;i++) {
				arr[i] = sc.nextInt();
				if(arr[i]%2==0) {
					 sum = sum + arr[i];
				}
			}
					System.out.println(sum);
				
				
			
			// TODO Auto-generated method stub

		}


		// TODO Auto-generated method stub

	}
