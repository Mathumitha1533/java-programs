package array_based_problem;

import java.util.Scanner;

public class Greaterthan10 {

	public static void main(String[] args) {
		
		// TODO Auto-generated method stub

		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int count = 0;
		int arr[] = new int[n];
		for(int i = 0;i<n;i++) {
			arr[i] = sc.nextInt();
			if(arr[i]>10) {
				
				count++;
				
				
			}
		}
			System.out.println(count);
			
		
		// TODO Auto-generated method stub

	

	}

}
